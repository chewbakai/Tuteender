<%- include('./partials/homeHeader.ejs') %> 
<body>
  <form action="/" method="GET">
    <div class="popular_courses">
      <div class="container">

        <div class="row justify-content-center">
          <div class="col-lg-5">
            <div class="main_title">
              <br>
              <br>
              <br>
              <h2 class="mb-3">Choose your Tutor</h2>
              <p>
                Learn from our tutors!
              </p>
            </div>
          </div>
        </div>
       
        <div class="row">
          <div class="col-lg-12">
              <div class="owl-carousel active_course">
                <div class="single_course">
                  <div class="course_head">
                    <img class="img-fluid" src="img/tutors/kai.jpg" alt="" />
                  </div>
                  <div class="course_content">
                    <span class="price">+</span>
                    <span class="tag mb-4 d-inline-block"><%- locals.status%></span>
                    <h4 class="mb-3">
                      <a href="tutorProfile.html"> <%- locals.tutorName%></a>
                    </h4>
                    <p>
                      <%- locals.description%>
                    </p>
                    <div
                      class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">
                      <a href="https://www.facebook.com/kyrahceniza"><i class="ti-email"></i></a>
                     
                    </div>
                  </div>
                </div>
                <div class="single_course">
                  <div class="course_head">
                    <img class="img-fluid" src="img/tutors/kirk.jpg" alt="" />
                  </div>
                  <div class="course_content">
                    <span class="price">+</span>
                    <span class="tag mb-4 d-inline-block"><% locals.status%></span>
                    <h4 class="mb-3">
                      <a href="tutorProfile.html"> <% locals.tutorName%></a>
                    </h4>
                    <p>
                      <% locals.rate%>
                    </p>
                    <div
                      class="course_meta d-flex justify-content-lg-between align-items-lg-center flex-lg-row flex-column mt-4">
                      <a href="https://www.facebook.com/kyrahceniza"><i class="ti-email"></i></a>
                     
                    </div>
                  </div>
                </div>


              
              </div>
            </div>
          </div>
          

         
      </div>
    </div>
   
      
    <script src="js/jquery-3.2.1.min.js"></script>
    <script src="js/popper.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="js/owl-carousel-thumb.min.js"></script>
    <script src="js/jquery.ajaxchimp.min.js"></script>
    <script src="js/mail-script.js"></script>
    <script src="js/theme.js"></script>
  </body>
</html>
