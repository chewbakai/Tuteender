<%- include('./partials/homeHeader.ejs') %> 

<body>
    <div class="container">
        <div class="row">
            <div class="col-lg-9">
                <div class="row">
                    <div class="col-sm-12">
                    <% tutors.forEach(tutor => { %>
                    <h1> <%= tutor.title %> </h1>
                    <p><img src="<%= tutors.imageURL%>" /></p>
                    <p>Rate: <%= tutor.rate %></p>
                    <p>Description: <%= tutor.description %></p>
                    <br>
                    <% }) %>
                    </div>
                </div>               
            </div>
         </div>
     </div>